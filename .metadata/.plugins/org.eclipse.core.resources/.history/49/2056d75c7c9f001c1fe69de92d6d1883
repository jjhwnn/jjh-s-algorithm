package com.javalec.base;

public class ValidParentheses {
	
	public static void main(String[] args) {
		
		
	}
	

public boolean isValid(String s) {
		// 향상된 for문과 toCharArray()메소드를 이용
	
	
		boolean check = false;
		s ="(]";
		
		if(s.length() == 1 || s.length() == 0) {
			check =false;
		}
		
		for(char a : s.toCharArray()) {
			
			switch(a) {
			
			case '(':
				break;
			case '{':
				break;
			case '[':
				break;
			case ')':
				break;
			case '}':
				break;
			case ']':
				break;
			
			}
			
			
		}
		
		

		
		return check;
		
	}
}
